<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Monospaced" />
    <option name="LINE_SPACING" value="1.0" />
    <option name="SECONDARY_FONT_FAMILY" value="Consolas" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="CSS" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="SHOW_QUICK_DOC_ON_MOUSE_OVER_ELEMENT" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="LiveEditOptions" useJBChromeExtension="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\b(vvm)todo\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\b(vv|vvm)q\b.*">
      <option name="FOREGROUND" value="b000dc" />
      <option name="BACKGROUND" value="fdff00" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff15" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\bVVM\b.*">
      <option name="FOREGROUND" value="760c" />
      <option name="BACKGROUND" value="f7ffbe" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="73ddff" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bVVR\b.*">
      <option name="FOREGROUND" value="93001c" />
      <option name="BACKGROUND" value="ffbbaf" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="ff9183" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bVVA\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="ff6d6b" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="fffa00" />
      <option name="ERROR_STRIPE_COLOR" value="ffffff" />
    </pattern>
    <filter name="VVM TODO">
      <pattern index="2" />
    </filter>
    <filter name="VVQ">
      <pattern index="3" />
    </filter>
    <filter name="VVR">
      <pattern index="5" />
    </filter>
    <filter name="VVM">
      <pattern index="4" />
    </filter>
    <filter name="ATTENTION">
      <pattern index="6" />
    </filter>
  </component>
</application>