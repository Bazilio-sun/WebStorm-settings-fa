<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\b(ss|vvm)todo\b.*">
      <option name="FOREGROUND" value="73bf" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\b(ss|vv|vvm)q\b.*">
      <option name="FOREGROUND" value="b000dc" />
      <option name="BACKGROUND" value="fdff00" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="ff15" />
    </pattern>
    <pattern useCustomColors="true" case-sensitive="true" pattern="\b(SS|VV)M\b.*">
      <option name="FOREGROUND" value="760c" />
      <option name="BACKGROUND" value="f7ffbe" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="73ddff" />
      <option name="ERROR_STRIPE_COLOR" value="54aae3" />
    </pattern>
    <filter name="VVM TODO">
      <pattern index="2" />
    </filter>
    <filter name="VVQ">
      <pattern index="3" />
    </filter>
    <filter name="VVM">
      <pattern index="4" />
    </filter>
  </component>
</application>